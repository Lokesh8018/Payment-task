{
  "expo": {
    "name": "Payment Task",
    "slug": "payment-task",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0055ff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lokesh.paymenttask"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0055ff"
      },
      "package": "com.lokesh.paymenttask"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    }
  }
}